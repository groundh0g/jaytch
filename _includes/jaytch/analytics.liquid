{% include jaytch/util-is-production.liquid %}

{% if isProduction and site.jaytch.analytics.provider and page.jaytch.analytics %}

{% case site.jaytch.analytics.provider %}
{% when "gauges" %}
  {% include jaytch/analytics/gauges.liquid %}
{% when "google" %}
  {% include jaytch/analytics/google.liquid %}
{% when "googleLegacy" %}
  {% include jaytch/analytics/googleLegacy.liquid %}
{% when "getclicky" %}
  {% include jaytch/analytics/getclicky.liquid %}
{% when "baidu" %}
  {% include jaytch/analytics/baidu.liquid %}
{% when "mixpanel" %}
  {% include jaytch/analytics/mixpanel.liquid %}
{% when "piwik" %}
  {% include jaytch/analytics/piwik.liquid %}
{% endcase %}

{% endif %}
